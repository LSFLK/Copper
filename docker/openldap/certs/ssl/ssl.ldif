dn:cn=config

add: olcTLSCipherSuite
olcTLSCipherSuite: TLSV1+RSA:!NULL
add: olcTLSCRLCheck
olcTLSCRLCheck: none
add: olcTLSVerifyClient
olcTLSVerifyClient: never
add: olcTLSCACertificateFile
olcTLSCACertificateFile: /container/service/slapd/assets/certs/openldap.csr
add: olcTLSCertificateFile
olcTLSCertificateFile: /container/service/slapd/assets/certs/openldap.crt
add: olcTLSCertificateKeyFile
olcTLSCertificateKeyFile: /container/service/slapd/assets/certs/openldap.key
