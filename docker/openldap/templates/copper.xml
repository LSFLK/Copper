<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE template SYSTEM "template.dtd">

<template>
<askcontainer>1</askcontainer>
<description>Copper Mail Accounts</description>
<icon>address-book.png</icon>
<invalid>0</invalid>
<rdn>cn</rdn>
<!--<regexp>^ou=People,o=.*,</regexp>-->
<title>Copper Mail Accounts</title>
<visible>1</visible>

<objectClasses>
<objectClass id="inetOrgPerson"></objectClass>
<objectClass id="evolutionperson"></objectClass>
<objectClass id="calEntry"></objectClass>
</objectClasses>

<attributes>
<attribute id="givenName">
	<display>First Name</display>
	<onchange>=autoFill(cn;%sn% %givenName%)</onchange>
	<order>1</order>
</attribute>
<attribute id="sn">
	<display>Last Name</display>
	<onchange>=autoFill(cn;%sn% %givenName%)</onchange>
	<order>2</order>
</attribute>
<attribute id="cn">
	<display>First Name</display>
	<order>3</order>
	<readonly>1</readonly>
</attribute>
<attribute id="mail">
	<display>Email</display>
	<icon>mail.png</icon>
	<order>6</order>
</attribute>
<attribute id="mobile">
	<display>Mobile</display>
	<order>10</order>
	<spacer>1</spacer>
</attribute>

<attribute id="ou">
	<display>Department</display>
	<order>25</order>
</attribute>

</attributes>
</template>
